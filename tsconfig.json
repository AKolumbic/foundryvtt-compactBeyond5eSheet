{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["DOM", "ES2020"],
    "types": [
      "@league-of-foundry-developers/foundry-vtt-types",
      "@league-of-foundry-developers/foundry-vtt-dnd5e-types"
    ],
    "esModuleInterop": true,
    "moduleResolution": "Node",
    "forceConsistentCasingInFileNames": true,
    "strictNullChecks": true,
    "sourceMap": true
  },
  "include": ["src"]
}
